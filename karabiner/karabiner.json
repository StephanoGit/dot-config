                                },
                                "to": [{ "shell_command": "osascript -e 'tell application \"System Events\" to keystroke \"#\"'" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "3",
                                    "modifiers": {
                                        "mandatory": ["left_option"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "shell_command": "osascript -e 'tell application \"System Events\" to keystroke \"£\"'" }],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
            "name": "Default profile",
